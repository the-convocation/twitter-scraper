{
  "name": "@the-convocation/twitter-scraper",
  "version": "0.1.4",
  "main": "dist/_module.js",
  "repository": "https://github.com/the-convocation/twitter-scraper.git",
  "author": "karashiiro <49822414+karashiiro@users.noreply.github.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "commit": "cz",
    "docs:generate": "typedoc --entryPoints src --entryPointStrategy expand --exclude **/*.test.ts",
    "docs:deploy": "yarn docs:generate && gh-pages -d docs",
    "format": "prettier --write \\\"src/**/*.ts\\\" \\\"test/**/*.ts\\\"",
    "prepare": "husky install",
    "test": "jest"
  },
  "dependencies": {
    "got": "^12.6.0",
    "got-scraping": "^3.2.13",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gh-pages": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0",
    "typedoc": "^0.24.1",
    "typescript": "^5.0.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
